{
  "name": "backend-config",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "src/libs/backend/config/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/src/libs/backend/config",
        "tsConfig": "src/libs/backend/config/tsconfig.lib.json",
        "packageJson": "src/libs/backend/config/package.json",
        "main": "src/libs/backend/config/src/index.ts",
        "assets": ["src/libs/backend/config/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "src/libs/backend/config/jest.config.cts"
      }
    }
  }
}
